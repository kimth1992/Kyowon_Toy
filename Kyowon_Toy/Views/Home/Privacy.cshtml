@model List<MemberModel>
@{
    //var dt = ViewData["dt"] as System.Data.DataTable;
   // var list = ViewData["list"] as List<MemberModel>;
}
<h1>@ViewData["Title"]</h1>

<p>DB 연동 및 데이터 전송 확인 페이지</p>

<table class = "table table-striped table-bordered">

    <thead>
        <tr>
            <th>no</th>
            <th>name</th>
            <th>sex</th>
        </tr>
    </thead>
    <tbody>
        @foreach(var member in Model)
        {
        <tr>
            <td>@member.No</td>
            <td>@member.Name</td>
            <td>@member.Sex</td>
    
        </tr>    
        }
        
    </tbody>


</table>


<div>
    <form id = "form1" method = "post" action = "/home/PrivacyInsert">
        <input name = "name" type = "text" value = "이름"/>
        <input name = "sex" type = "text" value = "성별"/>

        <button type = "submit" class = "btn btn-sm btn-primary">디비 삽입</button>
        </form>
</div>